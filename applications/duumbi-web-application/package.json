    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "react-router-dom": "6.23.1",
    "styled-components": "6.1.11"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@stylistic/eslint-plugin": "2.1.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/parser": "7.13.0",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "9.5.0",
