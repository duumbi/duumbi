{
  "name": "app-web-application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist/",
    "clean:env": "rm -f .env.production",
    "env": "doppler secrets download --no-file --format env > .env.production",
    "codegen": "rm -rf src/generated-sources/openapi; openapi-generator-cli generate -i ../../services/site-service/src/main/resources/api/openapi.yaml -g typescript-fetch -o src/generated-sources/openapi --template-dir custom-templates --additional-properties=supportsES6=true,npmVersion=10.2.3,typescriptThreePlus=true",
    "dev": "doppler run -- vite",
    "lint": "eslint . ",
    "preview": "vite preview",
    "build": "npm run clean && npm run codegen && npm run env && tsc && vite build --mode production && npm run clean:env"
  },
  "dependencies": {
    "@auth0/auth0-react": "2.2.4",
    "@types/new-relic-browser": "1.230.4",
    "antd": "5.19.1",
    "dayjs": "1.11.11",
    "dotenv": "16.4.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "react-router-dom": "6.24.1",
    "styled-components": "6.1.11"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.13.4",
    "@stylistic/eslint-plugin": "2.3.0",
    "@types/node": "^20.14.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/parser": "7.16.0",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "9.6.0",
    "rimraf": "6.0.1",
    "typescript": "5.5.3",
    "typescript-eslint": "7.16.0",
    "vite": "5.3.3",
    "vite-bundle-analyzer": "0.10.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
