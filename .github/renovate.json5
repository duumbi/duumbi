{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices",
    "schedule:weekly"
  ],
  "labels": ["dependencies", "renovate"],
  "dependencyDashboard": true,
  "prHourlyLimit": 3,
  "minimumReleaseAge": "3 days",
  "packageRules": [
    {
      "matchPackageNames": ["node", "npm"],
      "enabled": false
    }
  ]
}
